{
  "env": {
    "es6": true,
    "node": true,
    "jest/globals": true
  },
  "extends": ["airbnb-base", "plugin:jest/recommended"],
  "plugins": ["jest"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "comma-dangle": ["error", "never"],
    "max-len": ["error", { "code": 120 }]
  }
}
